{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs22.x"
    }
  },
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
